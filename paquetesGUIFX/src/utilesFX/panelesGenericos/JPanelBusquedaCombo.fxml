<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<fx:root id="AnchorPane" type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="lblNombre" text="Label" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Button fx:id="btnAnadir" contentDisplay="GRAPHIC_ONLY" maxWidth="28.0" minWidth="28.0" mnemonicParsing="false" prefWidth="28.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="0">
      <graphic>
        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../images/New16.gif" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <Button fx:id="btnBuscar" contentDisplay="GRAPHIC_ONLY" maxWidth="28.0" minWidth="28.0" mnemonicParsing="false" prefWidth="28.0" text="" GridPane.columnIndex="3" GridPane.rowIndex="0">
      <graphic>
        <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../images/Zoom16.gif" />
          </image>
        </ImageView>
      </graphic>
    </Button>
    <ComboBox fx:id="cmbCodigo" maxWidth="1.7976931348623157E308" GridPane.columnIndex="2" GridPane.rowIndex="0">
    </ComboBox>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" />
    <ColumnConstraints hgrow="NEVER" minWidth="28.0" prefWidth="28.0" />
    <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="10.0" />
    <ColumnConstraints hgrow="NEVER" minWidth="28.0" prefWidth="28.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</fx:root>
