<fx:root id="AnchorPane" prefHeight="205.0" prefWidth="429.0" styleClass="mainFxmlClass" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label prefWidth="800.0" style="-fx-background-color: #F5F7F8;" text="Cambio de contraseña" BorderPane.alignment="CENTER">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
   </top>
   <center>
      <GridPane prefHeight="267.0" prefWidth="911.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="452.0" minWidth="10.0" prefWidth="452.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="702.0" minWidth="10.0" prefWidth="459.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Contraseña actual" GridPane.halignment="RIGHT" />
            <CustomPasswordField fx:id="txtACTUAL" promptText="Contraseña actual" GridPane.columnIndex="1" />
            <Label text="Nueva contraseña" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
